@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    @await Component.InvokeAsync("_AdminHeadPartials")
</head>
<body>
    <div class="body-wrapper">
        <aside class="mdc-drawer mdc-drawer--dismissible mdc-drawer--open">
            <div class="mdc-drawer__header">
                <a href="../../index.html" class="brand-logo">
                    <img src="~/MaterialDash-Admin-1.0.0/template/assets/images/logo.svg" alt="logo">
                </a>
            </div>
            <br /><br />
            <div class="mdc-drawer__content">
                @await Component.InvokeAsync("_AdminSideBarPartials")
            </div>
        </aside>
        <div class="main-wrapper mdc-drawer-app-content">
            @await Component.InvokeAsync("_AdminHeaderPartials")
            <div class="page-wrapper mdc-toolbar-fixed-adjust">
                <main class="content-wrapper">
                    @RenderBody()
                </main>
                @await Component.InvokeAsync("_AdminFooterPartials")
            </div>
        </div>
    </div>
    @await Component.InvokeAsync("_AdminScriptPartials")
</body>
</html>
