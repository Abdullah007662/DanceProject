@model List<ResultPriceDTO>
<div class="price_section layout_padding">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <h1 class="price_taital">Our Price</h1>
            </div>
        </div>
        <div class="price_section_2">
            <div class="row">
                @foreach (var item in Model)
                {
                    <div class="col-md-6">
                        <h3 class="first_price_text">@item.Title</h3>
                        <div class="price_box">
                            <div class="taital_main">
                                <h4 class="rate_text"><span style="color: #f6815e;">₺</span>@item.Fiyat</h4>
                                <p class="month_text">/ Aylık</p>
                            </div>
                            <p class="dummy_text">
                                @item.Description
                            </p>
                            <div class="readmore_bt"><a href="#">Kayıt Ol</a></div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>